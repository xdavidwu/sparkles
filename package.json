{
  "private": "true",
  "type": "module",
  "scripts": {
    "compile-wasm": "cd helm-wasm && env GOOS=js GOARCH=wasm go build -o ../public/helm.wasm",
    "copy-wasm-exec": "cp \"$(go env GOROOT)\"/misc/wasm/wasm_exec.js src/vendor/",
    "wasm": "npm run compile-wasm && npm run copy-wasm-exec",
    "dev": "npm run wasm && vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only": "npm run wasm && vite build --base=\"${SPARKLES_BASE:-/}\"",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.1",
    "@codemirror/language": "^6.3.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.23.0",
    "@mdi/font": "^7.0.96",
    "@openapi-contrib/openapi-schema-to-json-schema": "^5.1.0",
    "@ragnarpa/quantity": "^0.2.0",
    "@tsmx/human-readable": "^2.0.1",
    "@vueuse/core": "^10.4.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "change-case": "^5.1.2",
    "chartjs-adapter-date-std": "^0.1.8",
    "codemirror-json-schema": "^0.7.0",
    "oidc-client-ts": "^3.0.0",
    "openapi-types": "^12.1.0",
    "parse-duration": "^1.0.2",
    "pinia": "^2.0.21",
    "semver": "^7.5.4",
    "vue": "^3.2.38",
    "vue-chartjs": "^5.2.0",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.5",
    "vuetify": "^3.4.1",
    "xxhashjs": "^0.2.2",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/golang-wasm-exec": "^1.15.0",
    "@types/json-schema": "^7.0.15",
    "@types/xxhashjs": "^0.2.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@vue/typescript-plugin": "^2.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.4",
    "vue-tsc": "^2.0.2"
  }
}
